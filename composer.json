{
  "name": "andrew72ru/symfony-telegram-bundle",
  "description": "Symfony 5 bundle for interact with Telegram",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew",
      "email": "andrew72ru@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4 || >=8.0",
    "ext-json": "*",
    "psr/log": "*",
    "symfony/dependency-injection": "^5.2",
    "symfony/event-dispatcher": "^5.2",
    "symfony/http-client": "^5.2",
    "symfony/http-foundation": "^5.2",
    "symfony/http-kernel": "^5.2",
    "symfony/property-access": "^5.2",
    "symfony/property-info": "^5.2",
    "symfony/serializer": "^5.2",
    "symfony/yaml": "^5.2"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2",
    "symfony/debug-pack": "*",
    "symfony/dotenv": "^5.2",
    "symfony/phpunit-bridge": "^5.2",
    "symfony/profiler-pack": "*"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "TelegramBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {}
}
