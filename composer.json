{
  "name": "andrew72ru/symfony-telegram-bundle",
  "description": "Symfony 4 bundle for interact with Telegram",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew",
      "email": "andrew72ru@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "psr/log": "^1.1",
    "symfony/dependency-injection": "^4.3",
    "symfony/flex": "^1.1",
    "symfony/framework-bundle": "^4.3",
    "symfony/http-foundation": "^4.3",
    "symfony/http-kernel": "^4.3"
  },
  "require-dev": {
    "codeception/c3": "^2.4",
    "codeception/codeception": "^3.1.2",
    "flow/jsonpath": "^0.4.0",
    "friendsofphp/php-cs-fixer": "^2.16",
    "fzaninotto/faker": "^1.8",
    "symfony/debug-pack": "^1.0",
    "symfony/dotenv": "^4.3",
    "symfony/profiler-pack": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
