{
  "name": "andrew72ru/symfony-telegram-bundle",
  "description": "Symfony 4 bundle for interact with Telegram",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andrew",
      "email": "andrew72ru@gmail.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.4",
    "ext-json": "*",
    "psr/log": "^1.1",
    "symfony/dependency-injection": "^4.4|^5",
    "symfony/event-dispatcher": "^4.4|^5",
    "symfony/http-client": "^4.4|^5",
    "symfony/http-foundation": "^4.4|^5",
    "symfony/http-kernel": "^4.4|^5",
    "symfony/property-access": "^4.4|^5",
    "symfony/property-info": "^4.4|^5",
    "symfony/serializer": "^4.4|^5",
    "symfony/yaml": "^5.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2",
    "fzaninotto/faker": "^1.9",
    "symfony/debug-pack": "^1.0",
    "symfony/dotenv": "^5.1",
    "symfony/phpunit-bridge": "^5.1",
    "symfony/profiler-pack": "^1.0"
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "TelegramBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {}
}
